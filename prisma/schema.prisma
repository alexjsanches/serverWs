// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PedidoNFatur {
  id                   Int       @id @default(autoincrement())
  empresa              Int
  nroUnico             Int
  nroNota              Int
  ordemCarga           Int?
  dataNegociacao       String
  previsaoEntrega      String?
  codTipoOper          Int
  descTipoOper         String
  tipMov               String
  entregaCliente       String
  codVendedor          Int
  vendedor             String
  codParceiro          Int
  razaoSocial          String
  pendente             String
  codProduto           Int
  descricaoProduto     String
  marca                String
  controle             String?
  qtdNegociada         Float
  qtdPendente          Float
  estoque              Float
  valorUnitario        Float
  valorTotal           Float
  valorPendente        Float
  mes                  String
  statusAguardando     String?
  previsaoEntregaFinal String?
  dataModificacao      DateTime?
}
